{
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"bracketSameLine": true,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"",
		"^@/commands/(.*)$",
		"",
		"^@/client",
		"",
		"^@/lib/(.*)$",
		"",
		"^@/events/(.*)$",
		"",
		"^@/modules/(.*)$",
		"",
		"^@/types",
		"^@/types/(.*)$",
		"",
		"^[./]"
	],
	"importOrderSortSpecifiers": true,
	"plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
